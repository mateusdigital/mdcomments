{
 "name": "mdcomments",
 "displayName": "mateusdigital - comments",
 "description": "Tool to be able to add header comments with file information",
 "repository": "https://github.com/mateusdigital/mdcomments",
 "website": "https://mateusdigital.com/projects/mdcomments",
 "version": "0.1.0",
 "build": 0,
 "engines": {
  "vscode": "^1.88.0"
 },
 "categories": [
  "Other"
 ],
 "activationEvents": [],
 "main": "./out/extension.js",
 "contributes": {
  "commands": [
   {
    "command": "mdcomments.singleLineComment",
    "title": "Single Line Comment"
   },
   {
    "command": "mdcomments.multiLineComment",
    "title": "Multi Line Comment"
   }
  ]
 },
 "scripts": {
  "vscode:prepublish": "npm run compile",
  "compile": "tsc -p ./",
  "watch": "tsc -watch -p ./",
  "pretest": "npm run compile && npm run lint",
  "lint": "eslint src --ext ts",
  "test": "vscode-test"
 },
 "devDependencies": {
  "@types/mocha": "^10.0.6",
  "@types/node": "18.x",
  "@types/vscode": "^1.88.0",
  "@typescript-eslint/eslint-plugin": "^7.4.0",
  "@typescript-eslint/parser": "^7.4.0",
  "@vscode/test-cli": "^0.0.8",
  "@vscode/test-electron": "^2.3.9",
  "eslint": "^8.57.0",
  "typescript": "^5.3.3"
 },
 "dependencies": {
  "json5": "^2.2.3"
 }
}